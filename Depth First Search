#include <iostream>
using namespace std;

const int MAXNODES = 10;
bool visited[MAXNODES];
int adjacencymatrix[MAXNODES][MAXNODES];

void DFS(int node, int n) {
    visited[node] = true;
    cout << "\n Indexed page: " << node;

    // Explore all adjacent nodes
    for (int i = 0; i < n; i++) {
        if (adjacencymatrix[node][i] == 1 && !visited[i]) {
            DFS(i, n);
        }
    }
}

int main() {
    int n, e;
    cout << "Enter number of web pages (max 10): ";
    cin >> n;

    // Initialize adjacency matrix and visited array
    for (int i = 0; i < n; i++) {
        visited[i] = false;
        for (int j = 0; j < n; j++) {
            adjacencymatrix[i][j] = 0;
        }
    }

    cout << "\nEnter number of hyperlinks: ";
    cin >> e;

    cout << "\nEnter links (from to):\n";
    for (int i = 0; i < e; i++) {
        int start, end;
        cin >> start >> end;

        if (start >= 0 && start < n && end >= 0 && end < n)
            adjacencymatrix[start][end] = 1;
        else
            cout << "Invalid link (" << start << ", " << end << ") ignored.\n";
    }

    int seed;
    cout << "\nEnter seed (start) page number: ";
    cin >> seed;

    cout << "\n\nStarting DFS web crawling from page: " << seed;
    DFS(seed, n);

    cout << endl;
    return 0;
}
